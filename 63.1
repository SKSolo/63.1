#include <iostream>
#include <fstream>
using namespace std;

void zadanie()
{
	string s;
	ifstream plik;
	
	plik.open("C:\\ciagi.txt");
	
	while (!plik.eof()){
        plik>>s; 
		 
        string s1,s2;
 		 s1 = (s.substr(0, s.length() / 2));
 		 s2 = (s.substr(s.length() / 2, s.length() / 2));
 		 
 		 
		if(s1==s2)
		{
			cout<<s1<<"||"<<s2<<"  "<<"Dwucykliczny \n";
		}
	  
    }
    plik.close();
    

   

	
}

int main(int argc, char** argv) {
	zadanie();
	return 0;
}
